{
  "name": "yuncms/yii2-note",
  "description": "The Yii Note module",
  "keywords": [
    "yii",
    "yii2-note",
    "note"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "xutl/yii2-highlightjs-widget": "~2.0",
    "xutl/yii2-select2-widget": "~1.0",
    "xutl/yii2-snowflake": "~1.0",
    "xutl/yii2-fmt-asset": "~1.0",
    "yuncms/yii2-user": "~2.0",
    "yuncms/yii2-collection": "~1.0.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\note\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "note",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/note/messages"
      },
      "migrationNamespace": "yuncms\\note\\migrations",
      "backend": {
        "class": "yuncms\\note\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\note\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
